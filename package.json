{
  "name": "python-language",
  "version": "0.0.0",
  "description": "Typescript utilities for generating and processing trees of the Python abstract syntax grammar",
  "author": "Phillip Dupuis (https://github.com/phillipdupuis)",
  "license": "GPL-v3",
  "homepage": "https://github.com/phillipdupuis/python-language",
  "repository": {
    "type": "git",
    "url": "https://github.com/phillipdupuis/python-language"
  },
  "bugs": {
    "url": "https://github.com/phillipdupuis/python-language/issues"
  },
  "keywords": [
    "python",
    "webassembly"
  ],
  "type": "module",
  "main": "./dist/python-language.umd.cjs",
  "module": "./dist/python-language.js",
  "types": "./dist/main.d.ts",
  "exports": {
    ".": {
      "import": "./dist/python-language.js",
      "require": "./dist/python-language.umd.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "test": "vitest --ui --coverage",
    "test:ci": "vitest",
    "test:coverage": "vitest --coverage",
    "build": "tsc && vite build",
    "preview": "vite preview"
  },
  "dependencies": {
    "pyodide": "^0.23.2"
  },
  "devDependencies": {
    "@types/node": "^20.1.0",
    "@vitest/coverage-c8": "^0.31.0",
    "@vitest/ui": "^0.31.0",
    "prettier": "^2.8.8",
    "typescript": "^5.0.2",
    "vite": "^4.3.2",
    "vite-plugin-dts": "^2.3.0",
    "vitest": "^0.31.0"
  }
}
